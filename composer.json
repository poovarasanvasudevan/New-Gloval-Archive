{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "barryvdh/laravel-ide-helper": "^2.2",
    "barryvdh/laravel-debugbar": "^2.2",
    "laravelcollective/bus": "^5.2",
    "maatwebsite/excel": "~2.1.0",
    "doctrine/dbal": "~2.3",
    "nayjest/grids": "^1.3.1",
    "laravelcollective/html": "^5",
    "laracasts/flash": "^2.0",
    "fedeisas/laravel-mail-css-inliner": "~1.5",
    "thetispro/laravel5-setting": "dev-master",
    "barryvdh/laravel-dompdf": "0.6.*",
    "laracasts/utilities": "~2.0",
    "miroc/laravel-adminer": "^4.2",
    "nwidart/laravel-config-writer": "dev-master",
    "thomaswelton/laravel-gravatar": "~1.0",
    "graham-campbell/github": "^4.4",
    "ixudra/curl": "6.*",
    "kozz/laravel-guzzle-provider": "~6.0",
    "emir/laravel-webartisan": "^1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/newhelper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
